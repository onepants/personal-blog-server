  <script src="/javascripts/jquery-1.11.0.js" type="text/javascript" charset="utf-8"></script>
  <script src="/javascripts/num.js" type="text/javascript" charset="utf-8"></script>
  <script type="text/javascript">

  	
  	
  	var num = 0;
  	//var s = $("#biao").val()
  	var s = 31;
	var timer = setInterval(function(){    				
    	num += 1;
    	$("#num").val(num);
	    if(num == s){
	    	clearInterval(timer);
	   	}
    },40)
    
    
   
  </script>
